	<script>	
		var charId = $("input[name='char-id']");
		var ftCharId = $("input[name='ft-char-id']");
		var assetId = $("input[name='assetId']");
		// filter the list
		const params = new URLSearchParams(window.location.search);
		// default to all themes
		const filter = params.get("filter") || "vl";
		if (filter == "cc")
			$(".theme").not("[data-has-cc]").hide();
		if (filter == "old")
			$(".theme").not("[data-isold]").hide();

		function redirect(tId, ccId = false) {
			if (filter == "cc") {
				window.location.href = `./character/creator?themeId=${ccId}&bs=`;
				return;
			}
			else if (filter == "theme")
			{
			window.location.href = `./go_full?tray=${tId}`;
			}
			else
			{
			window.location.href = `./studio_full?tray=${tId}`;
			}
		}
		function copyRedirect() {
			const id = charId.val();
			window.location.href = `./character/creator?original_asset_id=${id}`;
		}
