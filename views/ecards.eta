(function($) {
$(document).ready(function() {
	$(document).trigger('GoLite.stateChange', ['']);

	var next = 'step1', prev = '';

	$('#nav_links .next').click(function(e) {
		e.preventDefault();

		next && stepTo('#' + next);
	});
	$('#nav_links .prev').click(function(e) {
		e.preventDefault();

		prev && stepTo('#' + prev);
	});

	$('.section').waypoint({
		element: document.getElementById('element-waypoint'),
    	handler: function(direction) {
		var id = this.element.id;
		if (direction == 'up') {
			if (id == 'step1') {
				next = 'step1';
				prev = '';
				id = 'intro';
			} else if (id == 'step2') {
				next = 'step2';
				prev = 'intro';
				id = 'step1';
			} else if (id == 'step3') {
				next = 'step3';
				prev = 'step1';
				id = 'step2';
			} else if (id == 'step4') {
				next = 'step4';
				prev = 'step2';
				id = 'step3';
			}
		} else {
			if (id == 'step1') {
				next = 'step2';
				prev = 'intro';
			} else if (id == 'step2') {
				next = 'step3';
				prev = 'step1';
			} else if (id == 'step3') {
				next = 'step4';
				prev = 'step2';
			} else if (id == 'step4') {
				next = '';
				prev = 'step3';
			}
        }

		$('#nav_links').find('li').removeClass('on').end()
		.find('.' + id).addClass('on');
    	},
    	offset: '50%'
	});
	if ($('#login_bar').length) {
		$('#seperator0').waypoint(function(e) {
			$('#login_bar').slideDown();
		}, {offset: 'bottom-in-view', triggerOnce: true});

		$(document).bind('user.signup user.login', function(e) {
			GoLite.updateUserState(function(userState) {
				if (userState == 2) reloadCCList();
				$(document).trigger('GoLite.stateChange', ['']);
				$('#login_bar').slideUp();
				showNotice('You are now logged in to FlashThemes');
			});
		});
	}
    
	GoLite.updateUserState(function(userState) {
				if (userState == 2) { reloadCCList();
				$(document).trigger('GoLite.stateChange', ['']);
				}
			});
		
	jQuery('#movie_title, #movie_description').placeholder();
});
$(window).on("load", function (e) {$('#nav_links').fadeIn();})
})(jQuery);
</script>
